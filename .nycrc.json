{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": false,
  "reporter": [
    "text",
    "html",
    "lcov",
    "json"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test/**",
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "dist/**",
    "out/**",
    "node_modules/**",
    "**/*.d.ts"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage",
  "source-map": true,
  "instrument": true,
  "produce-source-map": true,
  "statements": 70,
  "lines": 70,
  "functions": 70,
  "branches": 60
}